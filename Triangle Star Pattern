import java.util.*;
public class Main{
    public static void main(String[]args){
    int i,s,j;
    for(i=1;i<=5;i++){
        for(s=1;s<=5-1;s++){
                System.out.print(" ");
            }
            for(j=1;j<=i*2-1;j++){
                System.out.print(" ");
            }
            System.out.println( );
        }
    }
}
